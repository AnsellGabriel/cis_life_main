<% batch_presenter = BatchPresenter.new(batch)%>

<div class="">
	<%= content_tag :span, "#{member}" %>
	<div class="sm-secondary-text <%= flex_justify_align('','center') %> gap-1">
			<div class="">
					<i class="bi bi-phone text-primary"></i>
					<%= content_tag :span, "#{member.mobile_number}" %>
			</div>
			<%= content_tag :span, '|' %>
			<div class="">
					<i class="bi bi-envelope text-primary"></i>
					<%= content_tag :span, "#{member.email}" %>
			</div>
	</div>
</div>
<div class="">

	<% if batch.status == 'recent' && batch.batch_health_decs.blank? %>
			<span class="badge bg-danger">No Health Declaration</span>
	<% end %>
	
</div>