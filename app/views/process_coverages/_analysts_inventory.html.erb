<div class="modal fade" id="benefitsModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-center" id="staticBackdropLabel">Analyst Inventory</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        
        <div class="accordion mb-2" id="accordionExample">
          <% current_date = Time.now %>
          <div class="accordion-item">
            <h2 class="accordion-header" id="headingOne">
              <button class="accordion-button collapsed text-secondary" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
              For Process
              </button>
            </h2>
            <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
              <div class="accordion-body">
                <table class="table">
                  <thead>
                    <th>Analyst</th>
                    <th>Day</th>
                    <th>Week</th>
                    <th>Month</th>
                    <th>Year</th>
                  </thead>
                  <tbody>
                    <% @analysts.each do |analyst| %>
                      <tr>
                        <td><%= analyst%></td>
                        <td>
                          <% start_date = current_date.beginning_of_day %>
                          <% end_date = current_date.end_of_day %>
                          <%= @for_process_coverages.where(processor: analyst, created_at: start_date..end_date).count %>
                        </td>
                        <td>
                          <% start_date = current_date.beginning_of_week %>
                          <% end_date = current_date.end_of_week %>
                          <%= @for_process_coverages.where(processor: analyst, created_at: start_date..end_date).count %>
                        </td>
                        <td>
                          <% start_date = current_date.beginning_of_month %>
                          <% end_date = current_date.end_of_month %>
                          <%= @for_process_coverages.where(processor: analyst, created_at: start_date..end_date).count %>
                        </td>
                        <td>
                          <% start_date = current_date.beginning_of_year %>
                          <% end_date = current_date.end_of_year %>
                          <%= @for_process_coverages.where(processor: analyst, created_at: start_date..end_date).count %>
                        </td>
                      </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>

            <div class="accordion-item">
              <h2 class="accordion-header" id="headingTwo">
                <button class="accordion-button collapsed text-success" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                  Approved
