<div class="card">
      <h5 class="card-header">Member Details</h5>
      <div class="card-body">
        <dl class="row">
          <dt class="col-sm-4">Route:</dt>
          <dd class="col-sm-8"><%= @process_claim.claim_route.humanize.titleize %></dd>
          <dt class="col-sm-4">Insurance:</dt>
          <dd class="col-sm-8"><%= @process_claim.agreement.plan %></dd>
          <dt class="col-sm-4">Name:</dt>
          <dd class="col-sm-8 text-primary"><%= @process_claim.claimable.get_fullname %></dd>
          <dt class="col-sm-4">Birthdate</dt>
          <dd class="col-sm-8"> <%= to_shortdate(@process_claim.claimable.member.birth_date) %></dd>
          <dt class="col-sm-4">Age:</dt>
          <dd class="col-sm-8"><%= @process_claim.age %></dd>
          <dt class="col-sm-4">Branch:</dt>
          <dd class="col-sm-8"><%= @process_claim.claimable.coop_branch %></dd>
          <dt class="col-sm-4">Mobile number:</dt>
          <dd class="col-sm-8"><%= @process_claim.claimable.member.mobile_number %></dd>
          <dt class="col-sm-4">Email:</dt>
          <dd class="col-sm-8"><%= @process_claim.claimable.member.email %> </dd>
          <% if @loan_batch.present? %>
            <dl class="row mt-3">
              <dt class="col-sm-4">Loan amount:</dt>
              <dd class="col-sm-8 text-primary"><%= to_curr @loan_batch.loan_amount %></dd>
              <dt class="col-sm-4">Loan effectivity date:</dt>
              <dd class="col-sm-8 text-primary"><%= to_shortdate @loan_batch.effectivity_date %></dd>
              <dt class="col-sm-4">Loan expiry date:</dt>
              <dd class="col-sm-8 text-primary"><%= to_shortdate @loan_batch.expiry_date %></dd>
            </dl>
          <% end %>
          <dt class="col-sm-4">Date Incident:</dt>
          <dd class="col-sm-8"><%= to_shortdate(@process_claim.date_incident) %> </dd>
          <dt class="col-sm-4">Claim Type:</dt>
          <dd class="col-sm-8"><%= @process_claim.claim_type %> </dd>
          
        </dl>
      </div>
    </div>