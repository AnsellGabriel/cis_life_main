<div class="mb-3">
  <ul class="timeline">
    <% @process_claim.process_track.order(created_at: :desc).each do |ct| %>
      <li class="mb-3">
        <p class="text-primary mb-1"> <%= Claims::ProcessClaim.get_route(ct.route_id).to_s.humanize.titleize %> <small class="text-muted"> <%= date_as_month_day_year ct.created_at %> </small></p>
        <% if ct.status.nil? %>
          <small>Performed by <%= ct.user %> </small>
        <% else %>
          <% if ct.status == "approved" %>
            <small class="text-success"><%= ct.status.humanize %></small>
          <% elsif ct.status == "denied" %>
            <small class="text-danger"><%= ct.status.humanize %></small>
          <% end %>
          <small>by <%= ct.user %></small>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
