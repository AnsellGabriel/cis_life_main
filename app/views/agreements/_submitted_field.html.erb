<%= render 'agreement_field', 
	name: 'Participants', 
	value: group_remit.batches.count, 
	margin: 'mb-2'
%>
<%= render 'agreement_field', 
	name: 'Effectivity Date', 
	value: group_remit.effectivity_date.strftime('%B %d, %Y'), 
	margin: ''
%>
<%= render 'agreement_field', 
	name: 'Expiry Date', 
	value: group_remit.expiry_date.strftime('%B %d, %Y'), 
	margin: 'mb-2'
%>
<%= render 'agreement_field', 
	name: 'Net Premium', 
	value: number_to_currency(group_remit.net_premium, unit: '₱', precision: 2), 
	margin: ''
%>
<%= render 'agreement_field', 
	name: 'Cooperative Service Fee', 
	value: number_to_currency(group_remit.coop_commission, unit: '₱', precision: 2), 
	margin: ''
%>